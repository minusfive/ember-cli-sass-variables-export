{
  "name": "ember-cli-sass-variables-export",
  "version": "1.1.4",
  "description": "Export Sass $variable values as JSON data to Ember Utilities, so they can be consumed from the rest of your app.",
  "keywords": [
    "ember",
    "ember-addon",
    "ember-cli",
    "ember-cli-addon",
    "ember-cli-sass",
    "emberjs",
    "emberjs-addon",
    "emberjs-frontend",
    "sass",
    "sass-functions",
    "node-sass"
  ],
  "license": "MIT",
  "contributors": [
    {
      "name": "Jorge Villalobos",
      "url": "https://github.com/minusfive/"
    },
    {
      "name": "Michael Munsie"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/minusfive/ember-cli-sass-variables-export",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "if [[ ! -d \"./dist\" ]]; then node_modules/.bin/ember build; fi; ember test",
    "test:all": "if [[ ! -d \"./dist\" ]]; then node_modules/.bin/ember build; fi && ember try:each",
    "preversion": "npm test",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "ember-cli-babel": "^7.1.0",
    "ember-cli-version-checker": "^3.1.2"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "^5.0.0",
    "ember-cli": "~3.8.1",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^5.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.24.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.0.1",
    "eslint-plugin-node": "^8.0.0",
    "loader.js": "^4.2.3",
    "qunit-dom": "^0.8.0",
    "sass": "^1.13.2"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
